act from 'react'\nimport home from '../assets/home.png'\nimport search from '../assets/search.png'\nimport sort from '../assets/sort.png'\nimport './SideBar.scss'\nimport PropTypes from 'prop-types'\nimport { NavLink, Route } from 'react-router-dom'\n\nconst SideBar = ({displayForm, clearSearchResults, showSort, showForm}) => {\n    return (\n      <nav>\n        <div className=\"sidebarIcons\">\n          <NavLink to='/'>\n            <img\n              className=\"homeIcon\"\n              src={home}\n              alt=\"home-icon\"\n              onClick={clearSearchResults}\n            />\n          </NavLink>\n          <Route exact path='/' render={ () => {\n            return (\n              <img\n                className=\"searchIcon\"\n                src={search}\n                alt=\"magnifying-glass\"\n                onClick={() => displayForm(\"showForm\", \"showForm\")}\n              />\n            );\n          }}\n          />\n          <Route exact path='/' render={ () => {\n            return <img \n            className=\"sortIcon\" \n            src={sort}\n            alt=\"up-and-down-arrows\"\n            onClick={() => displayForm('showSort', \"showSort\")}\n            />;\n          }}\n          />\n        </div>\n      </nav>\n    );\n}\n\nexport default SideBar\n","/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Header/Header.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Form/Form.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Card/Card.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Sort/Sort.js",["54","55"],"import React, { Component } from 'react'\nimport './Sort.scss'\n\nclass Sort extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      movies: [...this.props.movies],\n      sortedMovies: this.props.movies,\n      selected: ''\n    };\n  }\n\n  handleChange = (event) => {\n    this.setState({\n        selected: event.target.value\n    })\n      if (event.target.value === 'allMovies') {\n          this.props.displaySortedMovies(this.state.movies, this.state.movies);\n    } else if (event.target.value === \"high\") {\n        this.props.displaySortedMovies(this.state.movies,\n          this.sortMoviesHighToLow(\"average_rating\"))\n    } else if (event.target.value === \"low\") {\n        this.props.displaySortedMovies(this.state.movies,\n          this.sortMoviesLowToHigh(\"average_rating\"))\n    } else if (event.target.value === \"alphabetically\") {\n        this.props.displaySortedMovies(\n          this.state.movies, this.sortMoviesLowToHigh(\"title\")\n        );\n    } else if (event.target.value === 'reverseAlphabetically') {\n        this.props.displaySortedMovies(\n          this.state.movies, this.sortMoviesHighToLow(\"title\")\n        );\n    }\n  }\n\n  sortMoviesHighToLow = (key) => {\n    return this.state.sortedMovies.sort((a, b) => {\n        if (a[key] > b[key]) {\n            return -1\n        }\n    })\n  }\n\n  sortMoviesLowToHigh = (key) => {\n      return this.state.sortedMovies.sort((a, b) => {\n        if (a[key] < b[key]) {\n          return -1;\n        }\n      })\n  }\n\n\n  render() {\n    return (\n      <form className=\"sortDropDown\">\n        <label htmlFor=\"movies\">Sort by:</label>\n        <select name=\"movies\" id=\"movies\" value={this.state.selected} onChange={this.handleChange}>\n          <option value=\"allMovies\">View All Movies</option>\n          <option value=\"high\">Rating High to Low</option>\n          <option value=\"low\">Rating Low to High</option>\n          <option value=\"alphabetically\">Movie Title A to Z</option>\n          <option value=\"reverseAlphabetically\">Movie Title Z to A</option>\n        </select>\n      </form>\n    );\n  }\n}\n\nexport default Sort\n",{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":5