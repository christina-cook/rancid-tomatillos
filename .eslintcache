  })\n    })\n    .catch((error) => this.setState({ error: \"Something went wrong!\" }));\n  }\n\n  returnDate(date) {\n    return new Date(date).toLocaleDateString()\n  }\n\n  returnGenres(genres) {\n    return genres.join(', ')\n  }\n\n  returnMovieCosts(type) {\n    return new Intl.NumberFormat().format(type)\n  }\n\n  returnTrailer() {\n    const trailer = this.state.currentMovieTrailer.find(video => video.type === \"Trailer\")\n    return trailer\n  }\n\n  render() {\n      return (\n        <section className=\"movieDetails\">\n          {this.state.error && (\n            <h2 className=\"errorMessage\">{this.state.error}</h2>\n          )}\n\n          {this.state.currentMovie &&\n          <>\n            <MovieDetailsHeader currentMovie={this.state.currentMovie}/>\n            <div className=\"movieDetailsCard\">\n              <img\n                className=\"poster\"\n                src={this.state.currentMovie.poster_path}\n                alt={`${this.state.currentMovie.title}-poster`}\n              />\n              <span className=\"movieInfo\">\n                <h3 className=\"movieDetailsTitle\">Overview</h3>\n                <p className=\"movieOverview\">{this.state.currentMovie.overview}</p>\n                <div className=\"listDetails\">\n                  <ul className=\"movieDetailsList\">\n                    <li>Released</li>\n                    {this.state.currentMovie.runtime !== 0 && <li>Runtime</li>}\n                    {this.state.currentMovie.genres.length !== 0 && <li>Genre</li>}\n                    {this.state.currentMovie.budget !== 0 && <li>Budget</li>}\n                    {this.state.currentMovie.revenue !== 0 && <li>Revenue</li>}\n                    <li>Average Rating</li>\n                  </ul>\n                  <ul className=\"movieDetailsData\">\n                    <li>{this.returnDate(this.state.currentMovie.release_date)}</li>\n                    {this.state.currentMovie.runtime !== 0 && (\n                      <li>{this.state.currentMovie.runtime} minutes</li>\n                    )}\n                    {this.state.currentMovie.genres.length !== 0 && <li>{this.returnGenres(this.state.currentMovie.genres)}</li>}\n                    {this.state.currentMovie.budget !== 0 && <li>${this.returnMovieCosts(this.state.currentMovie.budget)}</li>}\n                    {this.state.currentMovie.revenue !== 0 && <li>${this.returnMovieCosts(this.state.currentMovie.revenue)}</li>}\n                    <li>{this.state.currentMovie.average_rating.toFixed(1)}</li>\n                  </ul>\n                </div>\n                {this.state.currentMovieTrailer.length !== 0 &&\n                  <div className=\"trailerContainer\">\n                  <ReactPlayer\n                    url={`https://youtu.be/${this.returnTrailer().key}`}\n                  />\n                </div>}\n              </span>\n            </div>\n          </>\n        }\n      </section>\n    )\n  }\n}\n\nexport default MovieDetails\n\nMovieDetails.propTypes = {\n  currentMovie: PropTypes.object\n}\n\n {\n   /* <span className=\"viewRatings\">\n          <img className=\"star\" src={star} alt=\"star\" />\n          <p className=\"movieRating\">{avgRating.toFixed(1)}</p>\n        </span> */\n }\n",["48","49"],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Data/API.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Movies/Movies.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/MovieDetailsHeader/MovieDetailsHeader.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/SideBar/SideBar.js",["50"],"import React from 'react'\nimport home from '../assets/home.png'\nimport search from '../assets/search.png'\nimport './SideBar.scss'\nimport PropTypes from 'prop-types'\nimport { NavLink, Route } from 'react-router-dom'\n\nconst SideBar = ({displayForm, clearSearchResults}) => {\n    return (\n      <nav>\n        <div className=\"sidebarIcons\">\n          <NavLink to='/'>\n            <img\n              className=\"homeIcon\"\n              src={home}\n              alt=\"home-icon\"\n              onClick={clearSearchResults}\n            />\n          </NavLink>\n          <Route exact path='/' render={ () => {\n            return (\n              <img\n                className=\"searchIcon\"\n                src={search}\n                alt=\"magnifying-glass\"\n                onClick={displayForm}\n              />\n            )\n          }}\n          />\n        </div>\n      </nav>\n    );\n}\n\nexport default SideBar\n","/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Header/Header.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Form/Form.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Card/Card.js",[],{"ruleId":"51","severity":1,"message":"52","line":5,"column":8,"nodeType":"53","messageId":"54","endLine":5,"endColumn":26},{"ruleId":"51","severity":1,"message":"55","line":8,"column":8,"nodeType":"53","messageId":"54","endLine":8,"endColumn":12},{"ruleId":"56","severity":1,"message":"57","line":107,"column":2,"nodeType":"58","messageId":"59","endLine":112,"endColumn":3},{"ruleId":"60","replacedBy":"61"},{"ruleId":"62","replacedBy":"63"},{"ruleId":"51","severity":1,"message":"64","line":5,"column":8,"nodeType":"53","messageId":"54","endLine":5,"endColumn":17},"no-unused-vars","'MovieDetailsHeader' is defined but never used.","Identifier","unusedVar","'Form' is defined but never used.","no-lone-blocks","Block is redundant.","BlockStatement","redundantBlock","no-native-reassign",["65"],"no-negated-in-lhs",["66"],"'PropTypes' is defined but never used.","no-global-assign","no-unsafe-negation"]