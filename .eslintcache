= {\n      movies: [],\n      currentMovie: {},\n      error: '',\n      loading: true,\n      showForm: false,\n      searchResults: [],\n    }\n  }\n\n  componentDidMount() {\n    getAllMovies()\n    .then(movies => this.setState({movies: movies.movies, loading: false}))\n    .catch(error => this.setState({error: 'Something went wrong!'}))\n  }\n\n  displayForm = () => {\n    this.setState({\n      showForm: !this.state.showForm\n    })\n    if (this.state.searchResults.length) {\n      this.setState({\n        searchResults: []\n      })\n    }\n  }\n\n  displaySearchResults = (movies) => {\n    this.setState({\n      searchResults: movies\n    })\n  }\n\n  clearSearchResults = () => {\n    this.setState({\n      searchResults: [],\n      showForm: false\n    })\n  }\n\n  render() {\n    return (\n      <main>\n        {this.state.error && (\n          <h2 className=\"errorMessage\">{this.state.error}</h2>\n        )}\n        {/* {this.state.loading && (\n            <h2>Loading...</h2>\n        )} */}\n\n      {this.state.movies.length > 0 && (\n        <>\n          <Route exact path='/' render={ () => {\n            return (\n              <>\n                <div className=\"mainPageContainer\">\n                  <SideBar\n                    displayForm={this.displayForm}\n                    clearSearchResults={this.clearSearchResults}\n                  />\n                  <div className=\"mainDisplayContainer\">\n                    <Header\n                      movies={this.state.movies}\n                      showForm={this.state.showForm}\n                      displaySearchResults={this.displaySearchResults}\n                    />\n                    <Movies\n                      movies={this.state.movies}\n                      searchResults={this.state.searchResults}\n                    />\n                  </div>\n                </div>\n              </>\n            );\n              }}\n            />\n          </>\n        )}\n\n        {this.state.currentMovie && (\n          <>\n            <Route\n              exact\n              path={'/movies/:id'}\n              render={ ({match}) => {\n                const id = parseInt(match.params.id)\n                return(\n                  <div className=\"movieDisplayContainer\">\n                    <SideBar displayForm={this.displayForm}/>\n                    <MovieDetails id={id} />\n                  </div>\n                )\n              }}\n            />\n          </>\n        )}\n\n      </main>\n    );\n  }\n}\n\nexport default App;\n",["52","53"],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/MovieDetails/MovieDetails.js",["54"],"import React, {Component} from 'react';\nimport './MovieDetails.scss';\nimport PropTypes from 'prop-types'\nimport MovieDetailsHeader from '../MovieDetailsHeader/MovieDetailsHeader'\nimport { getSingleMovieDetails } from '../Data/API'\nimport ReactPlayer from 'react-player'\n\nclass MovieDetails extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      currentMovie: null,\n      currentMovieTrailer: null,\n      id: this.props.id,\n      error: \"\",\n    };\n  }\n\n  componentDidMount() {\n    getSingleMovieDetails(this.state.id)\n    .then(data => {\n      this.setState({\n        currentMovie: data[0].movie,\n        currentMovieTrailer: data[1].videos\n      })\n    })\n    .catch((error) => this.setState({ error: \"Something went wrong!\" }));\n  }\n\n  returnDate(date) {\n    return new Date(date).toLocaleDateString()\n  }\n\n  returnGenres(genres) {\n    return genres.join(', ')\n  }\n\n  returnMovieCosts(type) {\n    return new Intl.NumberFormat().format(type)\n  }\n\n  returnTrailer() {\n    const trailer = this.state.currentMovieTrailer.find(video => video.type === \"Trailer\")\n    return trailer\n  }\n\n  render() {\n      return (\n        <section className=\"movieDetails\">\n          {this.state.error && (\n            <h2 className=\"errorMessage\">{this.state.error}</h2>\n          )}\n\n          {this.state.currentMovie &&\n          <>\n            <MovieDetailsHeader currentMovie={this.state.currentMovie}/>\n            <div className=\"movieDetailsCard\">\n              <img\n                className=\"poster\"\n                src={this.state.currentMovie.poster_path}\n                alt={`${this.state.currentMovie.title}-poster`}\n              />\n              <span className=\"movieInfo\">\n                <h3 className=\"movieDetailsTitle\">Overview</h3>\n                <p className=\"movieOverview\">{this.state.currentMovie.overview}</p>\n                <div className=\"listDetails\">\n                  <ul className=\"movieDetailsList\">\n                    <li>Released</li>\n                    {this.state.currentMovie.runtime !== 0 && <li>Runtime</li>}\n                    {this.state.currentMovie.genres.length !== 0 && <li>Genre</li>}\n                    {this.state.currentMovie.budget !== 0 && <li>Budget</li>}\n                    {this.state.currentMovie.revenue !== 0 && <li>Revenue</li>}\n                    <li>Average Rating</li>\n                  </ul>\n                  <ul className=\"movieDetailsData\">\n                    <li>{this.returnDate(this.state.currentMovie.release_date)}</li>\n                    {this.state.currentMovie.runtime !== 0 && (\n                      <li>{this.state.currentMovie.runtime} minutes</li>\n                    )}\n                    {this.state.currentMovie.genres.length !== 0 && <li>{this.returnGenres(this.state.currentMovie.genres)}</li>}\n                    {this.state.currentMovie.budget !== 0 && <li>${this.returnMovieCosts(this.state.currentMovie.budget)}</li>}\n                    {this.state.currentMovie.revenue !== 0 && <li>${this.returnMovieCosts(this.state.currentMovie.revenue)}</li>}\n                    <li>{this.state.currentMovie.average_rating.toFixed(1)}</li>\n                  </ul>\n                </div>\n                {this.state.currentMovieTrailer.length !== 0 &&\n                  <div className=\"trailerContainer\">\n                  <ReactPlayer\n                    url={`https://youtu.be/${this.returnTrailer().key}`}\n                  />\n                </div>}\n              </span>\n            </div>\n          </>\n        }\n      </section>\n    )\n  }\n}\n\nexport default MovieDetails\n\nMovieDetails.propTypes = {\n  currentMovie: PropTypes.object\n}\n\n {\n   /* <span className=\"viewRatings\">\n          <img className=\"star\" src={star} alt=\"star\" />\n          <p className=\"movieRating\">{avgRating.toFixed(1)}</p>\n        </span> */\n }\n","/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Data/API.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/Movies/Movies.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/MovieDetailsHeader/MovieDetailsHeader.js",[],"/Users/kelsiebesinger/turing/3module/projects/rancid-tomatillos/src/SideBar/SideBar.js",["55"],"import React from 'react'\nimport home from '../assets/home.png'\nimport search from '../assets/search.png'\nimport './SideBar.scss'\nimport PropTypes from 'prop-types'\nimport { NavLink, Route } from 'react-router-dom'\n\nconst SideBar = ({displayForm, clearSearchResults}) => {\n    return (\n      <nav>\n        <div className=\"sidebarIcons\">\n          <NavLink to='/'>\n            <img\n              className=\"homeIcon\"\n              src={home}\n              alt=\"home-icon\"\n              onClick={clearSearchResults}\n            />\n          </NavLink>\n          <Route exact path='/' render={ () => {\n            return (\n              <img\n                className=\"searchIcon\"\n                src={search}\n                alt=\"magnifying-glass\"\n                onClick={displayForm}\n              />\n            )\n          }}\n          />\n        </div>\n      </nav>\n    );\n}\n\nexport default Si