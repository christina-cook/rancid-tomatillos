y(searchResults) ||\n          determineMoviesToDisplay(movies)\n        }}\n    </section>\n  )\n}\n\nexport default Movies;\n\n Movies.propTypes = {\n   movies: PropTypes.array,\n   displayMovieDetails: PropTypes.func\n };\n","/Users/christinacook/turing/mod3/projects/proj-1/rancid-tomatillos/src/MovieDetailsHeader/MovieDetailsHeader.js",[],"/Users/christinacook/turing/mod3/projects/proj-1/rancid-tomatillos/src/Card/Card.js",[],"/Users/christinacook/turing/mod3/projects/proj-1/rancid-tomatillos/src/Data/API.js",[],"/Users/christinacook/turing/mod3/projects/proj-1/rancid-tomatillos/src/Form/Form.js",[],"/Users/christinacook/turing/mod3/projects/proj-1/rancid-tomatillos/src/Sort/Sort.js",["61","62"],"import React, { Component } from 'react'\nimport './Sort.scss'\n\nclass Sort extends Component {\n  constructor(props) {\n    super(props);\n    this.state = {\n      movies: [...this.props.movies],\n      sortedMovies: this.props.movies,\n      selected: ''\n    };\n  }\n\n  handleChange = (event) => {\n    this.setState({\n        selected: event.target.value\n    })\n      if (event.target.value === 'allMovies') {\n          this.props.displaySortedMovies(this.state.movies, this.state.movies);\n    } else if (event.target.value === \"high\") {\n        this.props.displaySortedMovies(this.state.movies,\n          this.sortMoviesHighToLow(\"average_rating\"))\n    } else if (event.target.value === \"low\") {\n        this.props.displaySortedMovies(this.state.movies,\n          this.sortMoviesLowToHigh(\"average_rating\"))\n    } else if (event.target.value === \"alphabetically\") {\n        this.props.displaySortedMovies(\n          this.state.movies, this.sortMoviesLowToHigh(\"title\")\n        );\n    } else if (event.target.value === 'reverseAlphabetically') {\n        this.props.displaySortedMovies(\n          this.state.movies, this.sortMoviesHighToLow(\"title\")\n        );\n    }\n  }\n\n  sortMoviesHighToLow = (key) => {\n    return this.state.sortedMovies.sort((a, b) => {\n        if (a[key] > b[key]) {\n            return -1\n        }\n    })\n  }\n\n  sortMoviesLowToHigh = (key) => {\n      return this.state.sortedMovies.sort((a, b) => {\n        if (a[key] < b[key]) {\n          return -1;\n        }\n      })\n  }\n\n\n  render() {\n    return (\n      <form className=\"sortDropDown\">\n        <label htmlFor=\"movies\">Sort by:</label>\n        <select name=\"movies\" id=\"movies\" value={this.state.selected} onChange={this.handleChange}>\n          <option value=\"allMovies\">View All Movies</option>\n          <option value=\"high\">Rating High to Low</option>\n          <option value=\"low\">Rating Low to High</option>\n          <option value=\"alphabetically\">Movie Title A to Z</option>\n          <option value=\"reverseAlphabetically\">Movie Title Z to A</option>\n        </select>\n      </form>\n    );\n  }\n}\n\nexport default Sort\n",{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":5,"column":8,"nodeType":"69","messageId":"70","endLine":5,"endColumn":26},{"ruleId":"67","severity":1,"message":"71","line":8,"column":8,"nodeType":"69","messageId":"70","endLine":8,"endColumn":12},{"ruleId":"72","severity":1,"message":"73","line":107,"column":2,"nodeType":"74","messageId":"75","endLine":112,"endColumn":3},{"ruleId":"67","severity":1,"message":"76","line":6,"column":8,"nodeType":"69","messageId":"70","endLine":6,"endColumn":17},{"ruleId":"77","severity":1,"message":"78","line":26,"column":34,"nodeType":"79","messageId":"80","endLine":26,"endColumn":36},{"ruleId":"77","severity":1,"message":"78","line":27,"column":50,"nodeType":"79","messageId":"80","endLine":27,"endColumn":52},{"ruleId":"77","severity":1,"message":"81","line":27,"column":50,"nodeType":"79","messageId":"80","endLine":27,"endColumn":52},{"ruleId":"77","severity":1,"message":"81","line":28,"column":36,"nodeType":"79","messageId":"80","endLine":28,"endColumn":38},{"ruleId":"82","